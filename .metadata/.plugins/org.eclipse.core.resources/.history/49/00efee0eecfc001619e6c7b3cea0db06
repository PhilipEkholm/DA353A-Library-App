package testing;

public class Prototype {
	public static void main(String[] args) {
		Prototype pt = new Prototype();
		pt.readFilesWithStream();
	}
	
	private void readFilesWithStream(){
		// Här skapas bl.a. lämplig datastruktur String str;
		String str;
		String[] values;
		
		while( ( str = reader.readLine() ) != null ) {
			values = str.split(";");
			lantagare = new Lantagare( values[ 0 ], values[ 1 ], values[ 2 ] ); 
			map.put( values[ 0 ], lantagare ); // nyckel = personnummer,
			// värde = Lantagare-objekt : // Här returneras bl.a. strukturen med Lantagare-objekt
	}
}
