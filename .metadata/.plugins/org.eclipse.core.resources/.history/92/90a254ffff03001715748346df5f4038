package library;

import javax.swing.JFrame;


public class main {
	public static void main(String[] args) {
		LoginView window = new LoginView();
		JFrame frame1 = new JFrame("LoginView");
		frame1.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		frame1.add(window);
		frame1.pack();
		frame1.setVisible(true);
	}
}
