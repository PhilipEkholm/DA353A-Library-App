package ws4;

import javax.swing.JOptionPane;

public class Lanekatalog {
	private int numberOfCards = 90;
	
	public boolean lan(int bokID, int kortNr, int pin){
		Lanekort aktuelltKort = new Lanekort();
		
		for(int i = 0; i < numberOfCards; i++){
			int nr = aktuelltKort.hamtaKortNr();
			if(nr == kortNr){
				if(aktuelltKort.kontrolleraPIN(pin)){
					
				}
				else{
					return false;
				}
			}
		}

		return false;
	}
}
